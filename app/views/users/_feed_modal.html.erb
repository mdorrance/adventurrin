<div id="photo-<%= feed.id %>" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <div class="row">
          <div class="col-sm-3">
            <%= image_tag render_profile_picture(feed), class: "img-responsive img-circle",
                        alt: "#{render_username(feed)} image" %>
          </div>
          <div class="col-sm-9 header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h3><%= render_username(feed) %><span class="pull-right">
              <a data-location="<%= render_location(feed) %>" data-latitude="<%= render_latitude(feed) %>" data-longitude="<%= render_longitude(feed) %>" data-image="<%= feed.images.standard_resolution.url %>" data-user="<%=current_user.id%>" class="create-adventure" href=""><i class="fa fa-heart-o"></i></a></span></h3>
            <h3><%= render_location(feed) %></h3>
          </div>
        </div>
      </div>
      <div class="modal-body">
        <iframe width="100%" height="100%" frameborder="0" style="border:0"
src="https://www.google.com/maps/embed/v1/search?q=<%=render_latitude(feed)%>,<%=render_longitude(feed)%>&key=AIzaSyA8EXRXA27O2SX5t7rY8wgLMFHZmiBqtWI" allowfullscreen></iframe>

        <div class="row">
          <%= render partial: 'yelp_results', locals: {feed: feed}%>
        </div>
      </div>
    </div>
  </div>
</div>
